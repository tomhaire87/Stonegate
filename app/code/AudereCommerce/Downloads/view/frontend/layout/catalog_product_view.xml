<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="product.info.details">
            <block class="AudereCommerce\Downloads\Block\Product\Downloads" name="downloads.tab"
                   template="AudereCommerce_Downloads::downloads.phtml" group="detailed_info" after="-">
                <arguments>
                    <argument name="title" translate="true" xsi:type="string">Downloads</argument>
                    <argument name="priority" xsi:type="string">10</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
