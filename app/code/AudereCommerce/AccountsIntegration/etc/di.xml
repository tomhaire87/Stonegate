<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="AudereCommerce\AccountsIntegration\Api\AccountManagementInterface" type="AudereCommerce\AccountsIntegration\Model\AccountManagement" />
    <preference for="AudereCommerce\AccountsIntegration\Api\AccountRepositoryInterface" type="AudereCommerce\AccountsIntegration\Model\AccountRepository" />
    <preference for="Magento\Framework\App\ResourceConnection" type="AudereCommerce\AccountsIntegration\Model\Framework\App\ResourceConnection" />
    <type name="Magento\Customer\Model\EmailNotification">
        <plugin name="accountsintegration_disable_customer_email" type="AudereCommerce\AccountsIntegration\Plugin\DisableCustomerEmail" />
    </type>
    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="auderecommerce/accountsintegration/all" xsi:type="object">AudereCommerce\AccountsIntegration\Console\All</item>
                <item name="auderecommerce/accountsintegration/import" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import</item>
                <item name="auderecommerce/accountsintegration/import/products" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Products</item>
                <item name="auderecommerce/accountsintegration/import/multiple-stock" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\MultipleStock</item>
                <item name="auderecommerce/accountsintegration/import/shipments" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Shipments</item>
                <item name="auderecommerce/accountsintegration/import/customers" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Customers</item>
                <item name="auderecommerce/accountsintegration/import/customers/customer" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Customers\Customer</item>
                <item name="auderecommerce/accountsintegration/import/customers/special-prices" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Customers\SpecialPrices</item>
                <item name="auderecommerce/accountsintegration/import/customers/discounts" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Import\Customers\Discounts</item>
                <item name="auderecommerce/accountsintegration/export" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Export</item>
                <item name="auderecommerce/accountsintegration/export/orders" xsi:type="object">AudereCommerce\AccountsIntegration\Console\Export\Orders</item>
            </argument>
        </arguments>
    </type>
</config>
