<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="page.main.title" destination="content" after="print.logo" />
        <referenceBlock name="logo" remove="true" />
        <referenceContainer name="breadcrumb.title" remove="true" />
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="print.logo" template="Stonegate_Base::content/print_logo.phtml" before="-" />
        </referenceContainer>
    </body>
</page>
